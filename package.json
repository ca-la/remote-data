{
  "name": "@cala/remote-data",
  "version": "1.0.0",
  "main": "dist/remote-data.js",
  "typings": "dist/remote-data.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run tslint && npm run prettier && ava",
    "tslint": "tslint -c tslint.json --project tsconfig.json './src/**/*.ts'",
    "prettier": "prettier --list-different './src/**/*.ts'",
    "prettier:fix": "prettier --write './src/**/*.ts'",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "CALA <devops@ca.la>",
  "license": "MPL-2.0",
  "devDependencies": {
    "ava": "1.0.0-beta.8",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git@github.com:ca-la/remote-data.git"
  },
  "bugs": {
    "url": "https://github.com/ca-la/remote-data/issues"
  },
  "homepage": "https://github.com/ca-la/remote-data#readme",
  "description": "Algebraic Data Type for Remote Data",
  "tags": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "keywords": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
